CREATE TABLE Artist (
    ID VARCHAR(100) NOT NULL,
    name VARCHAR(100) NOT NULL,
    url VARCHAR(200) NOT NULL,
    iconUrl VARCHAR(200) NOT NULL,
    timestamp DATETIME NOT NULL,
    PRIMARY KEY (ID)
);

CREATE TABLE Relate (
    relateId INT NOT NULL AUTO_INCREMENT,
    name1 VARCHAR(100) NOT NULL,
    name2 VARCHAR(100) NOT NULL,
    PRIMARY KEY (relateId),
    FOREIGN KEY (name1) REFERENCES Artist(ID) ON DELETE CASCADE,
    FOREIGN KEY (name2) REFERENCES Artist(ID) ON DELETE CASCADE
);